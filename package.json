{
    "name": "shopify-hydrogen-starter-with-react-redux",
    "description": "Custom store starter for Shopify & Hydrogen with React, Redux, SCSS, ESLint, Stylelint & Prettier",
    "version": "1.0.0",
    "license": "MIT",
    "author": "Julien Boutet",
    "engines": {
        "node": ">=16.14.0"
    },
    "scripts": {
        "dev": "shopify hydrogen dev",
        "build": "shopify hydrogen build",
        "preview": "shopify hydrogen preview",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
        "lint-ts": "tsc --noEmit",
        "test": "WATCH=true vitest",
        "test:ci": "yarn build -t node && vitest run"
    },
    "devDependencies": {
        "@reduxjs/toolkit": "^1.8.6",
        "@shopify/cli": "^3.17.0",
        "@shopify/cli-hydrogen": "^3.17.0",
        "@shopify/prettier-config": "^1.1.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.7",
        "@types/react": "^18.0.21",
        "eslint": "^8.25.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-hydrogen": "^0.12.2",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "playwright": "^1.27.1",
        "postcss": "^8.4.17",
        "postcss-import": "^15.0.0",
        "postcss-preset-env": "^7.8.2",
        "prettier": "^2.7.1",
        "sass": "^1.55.0",
        "stylelint": "^14.13.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended": "^9.0.0",
        "stylelint-prettier": "^2.0.0",
        "stylelint-scss": "^4.3.0",
        "tailwindcss": "^3.1.8",
        "typescript": "^4.8.4",
        "vitest": "^0.24.1"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.0",
        "@shopify/hydrogen": "^1.5.0",
        "clsx": "^1.2.1",
        "graphql-tag": "^2.12.6",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.4",
        "react-use": "^17.4.0",
        "typographic-base": "^1.0.4",
        "vite": "^2.9.13"
